<p>Crea nuova Espansione </p>


<%= form_for :expansion, url: expansions_path do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :brand_id %>
  <%= f.collection_select :brand_id, Brand.all, :id, :name %>


  <%= f.submit "Create Expansion", class: 'btn btn-primary', style: "width:250px !important" %>
<% end %>